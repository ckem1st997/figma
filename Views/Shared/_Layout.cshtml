    <link href="~/fontawesome-pro-5.0.13/css/all.min.css" rel="stylesheet" />
    @*<link rel="stylesheet" href="~/vegas/vegas.css">*@
    @*<link href="~/vegas/vegas.min.css" rel="stylesheet" />*@
    <link href="https://cdnjs.cloudflare.com/ajax/libs/vegas/2.5.1/vegas.min.css" rel="stylesheet" />
    @*<link rel="stylesheet" href="~/slick-1.8.1/slick/slick.css">*@
    @*<link href="~/slick-1.8.1/slick/slick.min.css" rel="stylesheet" />*@
